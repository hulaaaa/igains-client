{
  "expo": {
    "name": "igains-client",
    "slug": "igains-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/system/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/system/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#06070A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/system/adaptive-icon.png",
        "backgroundColor": "#06070A"
      }
    },
    "web": {
      "favicon": "./assets/images/system/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "../fonts/regular.otf",
            "../fonts/regular-italic.otf",
            "../fonts/light.otf",
            "../fonts/light-italic.otf",
            "../fonts/bold.otf",
            "../fonts/bold-italic.otf",
          ]
        }
      ]
    ]
  }
}
